{
  "name": "dev-academy-2022-exercise",
  "version": "1.0.0",
  "description": "App for uploading and viewing CSV data, built using React, Express and SQLite.",
  "author": {
    "name": "Patrick Park",
    "url": "https://gitbub.com/Vrezerino"
  },
  "private": false,
  "license": "MIT",
  "keywords": [
    "app",
    "csv",
    "express",
    "javascript",
    "react",
    "sqlite",
    "typescript"
  ],
  "browserslist": [
    "last 7 versions",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "engines": {
    "node": ">=10.0",
    "npm": ">=6.0",
    "yarn": ">=1.1.0"
  },
  "main": "src/index.tsx",
  "proxy": "http://localhost:4001",
  "scripts": {
    "build": "node ./buildScript",
    "build-front": "react-scripts build",
    "eject": "react-scripts eject",
    "start-server": "nodemon server/server.ts --watch server/*",
    "start-front": "set PORT=3006 && react-scripts start",
    "start": "concurrently \"npm run start-server\" \"npm run start-front\" --kill-others --kill-others-on-fail",
    "test": "react-scripts test --env=jsdom"
  },
  "dependencies": {
    "axios": "0.21.2",
    "body-parser": "1.19.0",
    "cors": "2.8.5",
    "express": "4.17.1",
    "fs": "0.0.1-security",
    "helmet": "3.22.0",
    "knex": "0.20.13",
    "multer": "^1.4.4",
    "react": "16.13.1",
    "react-dom": "16.13.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.6",
    "@types/react": "16.9.34",
    "@types/react-dom": "16.9.6",
    "concurrently": "5.1.0",
    "nodemon": "2.0.3",
    "react-scripts": "3.4.1",
    "sqlite3": "4.1.1",
    "typescript": "3.8.3"
  }
}
